					<description>微信点菜</description>
					<musicUrl>http://$domain$/music/1.mp3</musicUrl>
					<hqMusicUrl>http://$domain$/music/1_hq.mp3</hqMusicUrl>
			</rule-->
	 </business>
</root> 	
